
run = "npx serve dist"
entrypoint = "src/pages/index.astro"
modules = ["nodejs-22"]
hidden = [".config", "package-lock.json"]

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-22_11"

[deployment]
build = ["npm", "run", "build"]
deploymentTarget = "static"
publicDir = "dist"

[workflows]
runButton = "Run"

[[workflows.workflow]]
name = "Run"
author = 22930014
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
